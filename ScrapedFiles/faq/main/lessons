    <script type="text/javascript">
        // rumble the CTA buttons
        var rumbleButton = $('.gumroad-button');

        rumbleButton.jrumble();
        rumbleButton.trigger('startRumble');

        var startRumble = function(){
            rumbleButton.trigger('startRumble');
            setTimeout(stopRumble, 300);
        };
        var stopRumble = function(){
            rumbleButton.trigger('stopRumble');
            setTimeout(startRumble, 4000);
        };
        startRumble();
    </script>


